#: kconf_update/kconf_update.cpp:933
msgctxt "main|"
msgid "KDE Tool for updating user configuration files"
msgstr "КДЕ‑ова алатка за ажурирање корисничких поставних фајлова"

#: kconf_update/kconf_update.cpp:935
msgctxt "main|"
